#! /usr/bin/env python
# encoding: utf-8

from params import *
from util import *

def div(input, out1, out2, params_list = params_list):

	print(input, out1, out2)

	f = open(input, 'r')
	o1 = open(out1, 'w')
	o2 = open(out2, 'w')

	for line in f.readline():
		print(line)

		p = [eval(s) for s in line.split('\t')]
		r = p[len(params_list) + 1:]

		o1.write(str(r[0]) + '\n')
		o2.write(str(r[1]) + '\n')


if __name__ == '__main__':
	import sys

	div(sys.argv[1], sys.argv[2], sys.argv[3])
	
